{
  "name": "protonvpn_trial_bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/app.js --unhandled-rejections=strict",
    "dev": "concurrently -k -p \"[{name}]\" -n \"tsc-server,tsc-static,nodemon\" -c \"cyan.bold,green.bold,purple.bold\" \"npm:watch-ts\" \"npm:watch-static\" \"npm:watch-node\"",
    "build": "tsc -p tsconfig.json && npm run build-bundle-js && npm run copy-static-assets",
    "build-bundle-js": "tsc -p tsconfig.browserify.json && mkdirp ./dist/public/js/ && browserify ./dist/temp/public/js/index.js | npx babel --no-babelrc --presets=@babel/preset-env | uglifyjs --compress --mangle -o ./dist/public/js/bundle.js && rimraf ./dist/temp",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-ts": "tsc -w",
    "watch-static": "ts-node staticWatcher.ts",
    "watch-node": "nodemon dist/app.js -i dist/public/ -i dist/temp/ --unhandled-rejections=strict"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^7.0.3",
    "crypto-random-string": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.3",
    "puppeteer": "^3.0.1",
    "socket.io": "^2.5.0",
    "socket.io-client": "^2.3.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@types/ejs": "^3.0.2",
    "@types/express": "^4.17.6",
    "@types/express-handlebars": "^3.1.0",
    "@types/materialize-css": "^1.0.7",
    "@types/node": "^13.13.1",
    "@types/puppeteer": "^2.0.1",
    "@types/shelljs": "^0.8.7",
    "axios": "^0.21.4",
    "chokidar": "^3.4.0",
    "concurrently": "^7.0.0",
    "del": "^5.1.0",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  }
}
